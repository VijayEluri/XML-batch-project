        <xs:attribute form="unqualified" name="uri" type="xs:string">
          <xs:annotation>
            <xs:documentation><![CDATA[ 
				The URI of the call
			]]></xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="HttpType">
    <xs:complexContent>
      <xs:extension base="app0:XmlObjectType">
        <xs:choice maxOccurs="unbounded" minOccurs="0">
          <xs:element ref="get"/>
          <xs:element ref="post"/>
          <xs:element ref="call"/>
        </xs:choice>
        <xs:attribute form="unqualified" name="multiThreaded" type="xs:boolean">
          <xs:annotation>
            <xs:documentation><![CDATA[ 
				Allow the client to run in the background
			]]></xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute form="unqualified" name="timeout" type="xs:integer">
          <xs:annotation>
            <xs:documentation><![CDATA[ 
				The maximum time the client will wait
			]]></xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute form="unqualified" name="encode" type="xs:string">
          <xs:annotation>
            <xs:documentation><![CDATA[ 
				Set the string encoding charater set
			]]></xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute form="unqualified" name="trustStore" type="xs:string">
          <xs:annotation>
            <xs:documentation><![CDATA[ 
				File containing certificates of trusted sites
			]]></xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute form="unqualified" name="cookiePolity" type="xs:string">
          <xs:annotation>
            <xs:documentation><![CDATA[ 
				Set the cookie policy.
			]]></xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="AttributeType">
