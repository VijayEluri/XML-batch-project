<?xml version="1.0" encoding="UTF-8"?>
<xs:schema attributeFormDefault="qualified"
  elementFormDefault="qualified" id="query"
  targetNamespace="edu.bxml.http" xmlns="edu.bxml.http"
  xmlns:app0="com.browsexml.core" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:import namespace="com.browsexml.core" schemaLocation="com_browsexml_core.xsd"/>
  <xs:complexType mixed="true" name="ParameterType">
    <xs:complexContent>
      <xs:extension base="app0:XmlObjectType">
        <xs:choice maxOccurs="unbounded" minOccurs="0"/>
        <xs:attribute form="unqualified" name="value" type="xs:string">
          <xs:annotation>
            <xs:documentation><![CDATA[ 
				Set the value of the variable
			]]></xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute form="unqualified" name="type" type="xs:string">
          <xs:annotation>
            <xs:documentation><![CDATA[ 
				Set the type of the variable
			]]></xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute form="unqualified" name="key" type="xs:string">
          <xs:annotation>
            <xs:documentation><![CDATA[ 
				Set the name of the variable
			]]></xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="GetType">
    <xs:complexContent>
      <xs:extension base="app0:XmlObjectType">
        <xs:choice maxOccurs="unbounded" minOccurs="0">
          <xs:element ref="parameter"/>
        </xs:choice>
        <xs:attribute form="unqualified" name="http" type="xs:string">
          <xs:annotation>
            <xs:documentation><![CDATA[ 
				the name of the 'Http' object.  
 @param http
			]]></xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute form="unqualified" name="followRedirects" type="xs:boolean">
          <xs:annotation>
            <xs:documentation><![CDATA[ 
				If the page redirects, go to the redirection
			]]></xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute form="unqualified" name="url" type="xs:string">
          <xs:annotation>
            <xs:documentation><![CDATA[ 
				The URL of the page to get
			]]></xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute form="unqualified" name="newSymbolTable" type="xs:boolean">
          <xs:annotation>
            <xs:documentation><![CDATA[ 
				All predefined symbols are hidden.
 @param newSymbolTable
			]]></xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute form="unqualified" name="toFile" type="xs:string">
          <xs:annotation>
            <xs:documentation><![CDATA[ 
				Save the response in a file
 
 @param file
			]]></xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute form="unqualified" name="tempFilePrefix" type="xs:string">
          <xs:annotation>
            <xs:documentation><![CDATA[ 
				Prefix for a temporary file name
 @param toTemp
			]]></xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute form="unqualified" name="tempFileSuffix" type="xs:string">
          <xs:annotation>
            <xs:documentation><![CDATA[ 
				Suffix for a temporary file name
 @param toTempL
			]]></xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute form="unqualified" name="print" type="xs:boolean">
          <xs:annotation>
            <xs:documentation><![CDATA[ 
				Print the response to standard error
			]]></xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute form="unqualified" name="parse" type="xs:boolean">
          <xs:annotation>
            <xs:documentation><![CDATA[ 
				Interpret the results of the get; that is, assume the file is a 
 browseXML document and execute it.  
 @see setNewSymbolTable
 @param parse
			]]></xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="PostType">
    <xs:complexContent>
      <xs:extension base="app0:XmlObjectType">
        <xs:choice maxOccurs="unbounded" minOccurs="0"/>
        <xs:attribute form="unqualified" name="timeout" type="xs:integer">
          <xs:annotation>
            <xs:documentation><![CDATA[ 
				NO COMMENT
			]]></xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute form="unqualified" name="followRedirects" type="xs:boolean">
          <xs:annotation>
            <xs:documentation><![CDATA[ 
				If the page redirects, go to the redirection
			]]></xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute form="unqualified" name="url" type="xs:string">
          <xs:annotation>
            <xs:documentation><![CDATA[ 
				The URL of the page to get
			]]></xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute form="unqualified" name="print" type="xs:boolean">
          <xs:annotation>
            <xs:documentation><![CDATA[ 
				Print the response to standard error
			]]></xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute form="unqualified" name="parse" type="xs:boolean">
          <xs:annotation>
            <xs:documentation><![CDATA[ 
				Assuem the response is a browseXML document and execute it
			]]></xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="CallType">
    <xs:complexContent>
      <xs:extension base="app0:XmlObjectType">
        <xs:choice maxOccurs="unbounded" minOccurs="0">
          <xs:element ref="parameter"/>
        </xs:choice>
        <xs:attribute form="unqualified" name="method" type="xs:string">
          <xs:annotation>
            <xs:documentation><![CDATA[ 
				The Method of the call
			]]></xs:documentation>
          </xs:annotation>
